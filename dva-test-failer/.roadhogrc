{
  "entry": "src/index.js",
  "disableCSSModules": false,
  "less": false,
  "publicPath": "/",
  "autoprefixer": null,
  "proxy": {
    "/api": {
      "target": "http://jsonplaceholder.typicode.com/",
      "changeOrigin": true,
      "pathRewrite": { "^/api" : "" }
    }
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
          [ "import",
            { "libraryName": "antd", "style": "css" },
            { "libraryName": "antd-mobile"}
          ],
        "dva-hmr",
        "transform-runtime",
        ["module-resolver", {
          "alias": {
            "dva": "dva-react-router-3"
          }
        }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
          [ "import",
            { "libraryName": "antd", "style": "css" },
            { "libraryName": "antd-mobile"}
          ],
        "transform-runtime",
        ["module-resolver", {
          "alias": {
            "dva": "dva-react-router-3"
          }
        }]
      ]
    }
  }
}
