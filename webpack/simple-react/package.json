    "serviceworker-webpack-plugin": "^1.0.1",
    "speed-measure-webpack-plugin": "^1.3.3",
    "style-loader": "^1.2.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.1",
    "terser-webpack-plugin": "^3.0.1",
    "ts-loader": "^7.0.2",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
